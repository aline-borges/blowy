{
  "expo": {
    "name": "blowy",
    "slug": "blowy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/favicon/favicon.png",
    "splash": {
      "image": "./assets/images/logo/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
